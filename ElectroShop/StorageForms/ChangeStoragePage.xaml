<Window x:Class="ElectroShop.StorageForms.ChangeStoragePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectroShop.StorageForms"
        mc:Ignorable="d"
        Title="InsertCompany" MinHeight="370" MinWidth="300" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <StackPanel>
        <Label Content="ID" Margin="3"/>
        <TextBox x:Name="ID" MaxLength="6" Margin="3"/>
        <Label Content="Name" Margin="3"/>
        <TextBox x:Name="Name" MaxLength="40" Margin="3"/>
        <Label Content="Count" Margin="3"/>
        <TextBox x:Name="Count" MaxLength="6" Margin="3"/>
        <Label Content="TypeID" Margin="3"/>
        <TextBox x:Name="TypeID" MaxLength="6" Margin="3"/>
        <Label Content="CompanyID" Margin="3"/>
        <TextBox x:Name="CompanyID" MaxLength="6" Margin="3"/>
        <Label Content="Price" Margin="3"/>
        <TextBox x:Name="Money" MaxLength="30" Margin="3"/>
        <Button x:Name="InsertNew" Content="Добавить" Margin="3" Click="InsertNew_Click"/>
        <Button x:Name="Cancel" Content="Отмена" Margin="3" Click="Cancel_Click"/>
    </StackPanel>
</Window>
